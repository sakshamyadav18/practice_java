import java.util.*;
public class main{
    public static void main(String[] args){
        Scanner input = new Scanner(System.in);
        System.out.print("Enter No. of Rows: ");
        int Row = input.nextInt();
        for(int i = 1; i<=Row; i++){
            for(int j = 1; j<=i; j++){
                System.out.print(j);
            }
            System.out.println(" ");
        }
    }
}
