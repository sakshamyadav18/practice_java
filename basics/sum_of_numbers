import java.util.*;
public class Main{
    public static void main(String[] args){
        Scanner input = new Scanner(System.in);
        System.out.print("Enter a ");
        int a = input.nextInt();
        System.out.print("Enter b ");
        int b = input.nextInt();
        System.out.println("Sum of a and b is "+(a+b));
    }
}
