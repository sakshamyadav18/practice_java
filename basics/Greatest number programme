import java.util.*;
public class Main{
    public static void main(String[] args){
        Scanner input = new Scanner(System.in);
        System.out.print("Enter a: ");
        int a = input.nextInt();
        System.out.print("Enter b: ");
        int b = input.nextInt();
        if(a>b){
            System.out.println("Greatest Number is " + a);
        }
        else{
            System.out.println("Greatest Number is " + b);
        }
    }
}
