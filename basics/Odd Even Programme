import java.util.*;
public class Main{
    public static void main(String[] args){
        Scanner input = new Scanner(System.in);
        System.out.print("Enter any number: ");
        int number = input.nextInt();
        if(number%2==0){
            System.out.print("No. is Even");
        }
        else{
            System.out.print("No. is Odd");
        }
    }
}
